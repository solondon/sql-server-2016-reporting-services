  <DataSets>
    <DataSet Name="SalesData">
      <Query>
        <DataSourceName>WideWorldImportersDW</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@StateProvince">
            <Value>=Parameters!States.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@BillToCustomer">
            <Value>=Parameters!BillToCustomer.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Employee">
            <Value>=Parameters!Employees.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT ci.[State Province]
     , cu.[Bill To Customer]
     , fs.[Stock Item Key]
     , fs.[Invoice Date Key]
     , fs.[Delivery Date Key]
     , fs.[Salesperson Key]
     , em.[Employee]
     , fs.[Description]
     , fs.[Total Excluding Tax]
     , fs.[Tax Amount]
     , fs.[Profit]
     , fs.[Total Including Tax]
  FROM [WideWorldImportersDW].[Fact].[Sale] fs
  JOIN [Dimension].[City] ci 
    ON fs.[City Key] = ci.[City Key]
  JOIN [Dimension].[Customer] cu
    ON fs.[Customer Key] = cu.[Customer Key]
  JOIN [Dimension].[Employee] em
    ON fs.[Salesperson Key] = em.[Employee Key]
WHERE ci.[State Province] = @StateProvince
  AND cu.[Bill To Customer] = @BillToCustomer
  AND em.[Employee] = @Employee
</CommandText>
      </Query>
